<script>
                    function addUp()
                    {
                        var price = 0;
                        for (var i = 1; i <= 15; i++)
                        {
                            price += (document.getElementById("input-"+i).value*parseFloat(document.getElementById("price-"+i).innerHTML.split(" ")[0].split("£")[1]));
                        }    
                        document.getElementById("total_work").innerHTML = price.toFixed(2);
                    }

                    function update(inputNo, inc)
                    {
                            tb = document.getElementById("input-"+inputNo);
                            currVal = tb.value;
                            if (isNaN(currVal))
                            {
                                document.getElementById("input-"+inputNo).value = 0;
                            }
                            else
                            {
                                if (inc)
                                {
                                    document.getElementById("input-"+inputNo).value++;
                                        
                                }   
                                else
                                {
                                    if (currVal > 0)
                                    {
                                        document.getElementById("input-"+inputNo).value--;
                                    }
                                    

                                } 
                            }
                            addUp();
                    }
                    </script>
                   
</body>
</html>

<?php
function cf_shortcode() {
    html_form_code();
 
    return ob_get_clean();
}


add_shortcode( 'sitepoint_contact_form', 'cf_shortcode' );
